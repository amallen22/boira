section#events
  h3 Upcoming events
  .row
    .col-xs-12.col-sm-12.col-md-6
      ul.list
        each event in eventsList
          li
            .date
              .day= event.day
              .month_year
                span= event.month
                span= event.year
            .place
              h4= event.place
              h5= event.location
            a.btn.btn-default.gold(href='#') Buy tickets
    .col-xs-12.col-sm-12.col-md-6
      iframe(width='100%', height='400', frameborder='0', style='border:0', src='https://www.google.com/maps/embed/v1/place?key=AIzaSyDCQxedbicT6PlFst0lH4pl3Q3MgwvLIR4\
      &q=Space+Needle,Seattle+WA', allowfullscreen='')